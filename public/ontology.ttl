#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/iotf#becauseOf
:becauseOf rdf:type owl:ObjectProperty ;
           rdfs:domain :FailureMode ;
           rdfs:range :FailureMode .


###  https://w3id.org/iotf#effectIs
:effectIs rdf:type owl:ObjectProperty ;
          rdfs:domain :FailureMode ;
          rdfs:range :FailureMode .


###  https://w3id.org/iotf#happensAt
:happensAt rdf:type owl:ObjectProperty ;
           rdfs:domain :FailureMode ;
           rdfs:range :FeatureKind .


###  https://w3id.org/iotf#hasDeviceType
:hasDeviceType rdf:type owl:ObjectProperty ;
               rdfs:range :FeatureKind .


###  https://w3id.org/iotf#hasRecoveryAction
:hasRecoveryAction rdf:type owl:ObjectProperty ;
                   rdfs:domain :FailureMode ;
                   rdfs:range :CompensatoryAction .


###  https://w3id.org/iotf#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:domain :FailureMode ;
            rdfs:range :PropertyValue .


###  https://w3id.org/iotf#isCausedBy
:isCausedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :FailureMode ;
            rdfs:range :FailureCause .


###  https://w3id.org/iotf#producesEffect
:producesEffect rdf:type owl:ObjectProperty ;
                rdfs:domain :FailureMode ;
                rdfs:range :FailureEffect .


###  http://www.w3.org/2004/02/skos/core#broader
<http://www.w3.org/2004/02/skos/core#broader> rdf:type owl:ObjectProperty .


###  http://www.w3.org/2004/02/skos/core#narrower
<http://www.w3.org/2004/02/skos/core#narrower> rdf:type owl:ObjectProperty .


###  https://saref.etsi.org/core/hasDeviceKind
<https://saref.etsi.org/core/hasDeviceKind> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <https://saref.etsi.org/core/hasFeatureKind> ;
                                            rdfs:domain :Device ;
                                            rdfs:range <https://saref.etsi.org/core/FeatureKind> ;
                                            rdfs:comment "Links a device to its kind, a feature kind. Kinds of devices describe models of devices, with common properties having the same value, common states being the same, common functions, and common services."@en ;
                                            rdfs:label "has device kind"@en .


###  https://saref.etsi.org/core/hasFeatureKind
<https://saref.etsi.org/core/hasFeatureKind> rdf:type owl:ObjectProperty .


###  https://saref.etsi.org/core/hasPropertyValue
<https://saref.etsi.org/core/hasPropertyValue> rdf:type owl:ObjectProperty ;
                                               rdfs:domain :Device ;
                                               rdfs:range <https://saref.etsi.org/core/PropertyValue> ;
                                               owl:propertyChainAxiom ( <http://www.w3.org/2004/02/skos/core#broader>
                                                                        <https://saref.etsi.org/core/hasPropertyValue>
                                                                      ) ;
                                               rdfs:comment "Links a feature kind, a feature of interest, or a property of interest, to a property value."@en ;
                                               rdfs:label "has property value"@en ;
                                               <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:hasPropertyValue."@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/iotf#hasFailureCode
:hasFailureCode rdf:type owl:DatatypeProperty ;
                rdfs:domain :FailureMode ;
                rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://org.semanticweb.owlapi/error#Error1
<http://org.semanticweb.owlapi/error#Error1> rdf:type owl:Class .


###  https://w3id.org/iotf#AudioOutputFault
:AudioOutputFault rdf:type owl:Class ;
                  rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#BLEFault
:BLEFault rdf:type owl:Class ;
          rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#BatteryFluctuation
:BatteryFluctuation rdf:type owl:Class ;
                    rdfs:subClassOf :PowerFailure .


###  https://w3id.org/iotf#BatteryLevel
:BatteryLevel rdf:type owl:Class ;
              rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#CPUUsage
:CPUUsage rdf:type owl:Class ;
          rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#Calibration
:Calibration rdf:type owl:Class ;
             rdfs:subClassOf :NonFailStop ;
             rdfs:comment "happens when sensor data may have offset or have a different gain from the ground truth values." .


###  https://w3id.org/iotf#CellularFault
:CellularFault rdf:type owl:Class ;
               rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#CommunicationFailure
:CommunicationFailure rdf:type owl:Class ;
                      rdfs:subClassOf :FailureMode .


###  https://w3id.org/iotf#CompensatoryAction
:CompensatoryAction rdf:type owl:Class ;
                    rdfs:comment "denotes actions that allows recovering a device from a given failure." .


###  https://w3id.org/iotf#ConnectivityFailure
:ConnectivityFailure rdf:type owl:Class ;
                     rdfs:subClassOf :FailureCause ;
                     rdfs:comment "represents failure cause related to the connectivity." .


###  https://w3id.org/iotf#Device
:Device rdf:type owl:Class ;
        owl:equivalentClass <https://saref.etsi.org/core/Device> .


###  https://w3id.org/iotf#Environnment
:Environnment rdf:type owl:Class ;
              rdfs:subClassOf :FailureCause ;
              rdfs:comment "represents failure cause related to the environment." .


###  https://w3id.org/iotf#EthernetFault
:EthernetFault rdf:type owl:Class ;
               rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#FailStop
:FailStop rdf:type owl:Class ;
          rdfs:subClassOf :FailureMode ;
          rdfs:comment "happens when a device stops functioning and is un-responsive to external requests." .


###  https://w3id.org/iotf#FailureCause
:FailureCause rdf:type owl:Class ;
              rdfs:comment "denotes the cause of a failure." .


###  https://w3id.org/iotf#FailureEffect
:FailureEffect rdf:type owl:Class ;
               rdfs:comment "represents the effects of a failure mode." .


###  https://w3id.org/iotf#FailureMode
:FailureMode rdf:type owl:Class ;
             rdfs:comment "a form of a dysfunction related to an IoTdevice." .


###  https://w3id.org/iotf#FeatureKind
:FeatureKind rdf:type owl:Class ;
             owl:equivalentClass <https://saref.etsi.org/core/FeatureKind> ;
             rdfs:comment "denotes a type of a market IoT device e.g., FIBARO light bulb." .


###  https://w3id.org/iotf#FirmwareUpdate
:FirmwareUpdate rdf:type owl:Class ;
                rdfs:subClassOf :CompensatoryAction .


###  https://w3id.org/iotf#FirmwareVersion
:FirmwareVersion rdf:type owl:Class ;
                 rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#HardwareFailure
:HardwareFailure rdf:type owl:Class ;
                 rdfs:subClassOf :FailureCause ;
                 rdfs:comment "represents failure cause related to the hardware." .


###  https://w3id.org/iotf#HardwareRestart
:HardwareRestart rdf:type owl:Class ;
                 rdfs:subClassOf :CompensatoryAction .


###  https://w3id.org/iotf#HardwareVersion
:HardwareVersion rdf:type owl:Class ;
                 rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#HighVariance
:HighVariance rdf:type owl:Class ;
              rdfs:subClassOf :NonFailStop ;
              rdfs:comment "A high variance failure appears when a deviceâ€™s state fluctuates back and forth more rapidly than the environment dictates." .


###  https://w3id.org/iotf#LowBattery
:LowBattery rdf:type owl:Class ;
            rdfs:subClassOf :PowerFailure .


###  https://w3id.org/iotf#MemoryUsage
:MemoryUsage rdf:type owl:Class ;
             rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#NFCFault
:NFCFault rdf:type owl:Class ;
          rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#NonFailStop
:NonFailStop rdf:type owl:Class ;
             rdfs:subClassOf :FailureMode ;
             rdfs:comment "failure relate to a response by the device that diverges from the desired device state." .


###  https://w3id.org/iotf#NonVolatileMemoryError
:NonVolatileMemoryError rdf:type owl:Class ;
                        rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#Omission
:Omission rdf:type owl:Class ;
          rdfs:subClassOf :CommunicationFailure .


###  https://w3id.org/iotf#Outlier
:Outlier rdf:type owl:Class ;
         rdfs:subClassOf :NonFailStop ;
         rdfs:comment "an outlier failure occurs when a device to report an incorrect state for a very brief period, usually theduration of just 1 poll." .


###  https://w3id.org/iotf#PowerFailure
:PowerFailure rdf:type owl:Class ;
              rdfs:subClassOf :FailureCause ;
              rdfs:comment "reprensents failure cause related to power e.g, battery level." .


###  https://w3id.org/iotf#PowerSource
:PowerSource rdf:type owl:Class ;
             rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#PropertyValue
:PropertyValue rdf:type owl:Class ;
               owl:equivalentClass <https://saref.etsi.org/core/PropertyValue> .


###  https://w3id.org/iotf#RadioFault
:RadioFault rdf:type owl:Class ;
            rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#RebootCount
:RebootCount rdf:type owl:Class ;
             rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#SoftwareFailure
:SoftwareFailure rdf:type owl:Class ;
                 rdfs:subClassOf :FailureCause ;
                 rdfs:comment "represents failure cause related to software." .


###  https://w3id.org/iotf#SoftwareRestart
:SoftwareRestart rdf:type owl:Class ;
                 rdfs:subClassOf :CompensatoryAction .


###  https://w3id.org/iotf#SoftwareUpdate
:SoftwareUpdate rdf:type owl:Class ;
                rdfs:subClassOf :CompensatoryAction .


###  https://w3id.org/iotf#SoftwareVersion
:SoftwareVersion rdf:type owl:Class ;
                 rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#Spike
:Spike rdf:type owl:Class ;
       rdfs:subClassOf :NonFailStop ;
       rdfs:comment "A spike fault happens when there is a rapid increase or decrease in the device state." .


###  https://w3id.org/iotf#StuckAt
:StuckAt rdf:type owl:Class ;
         rdfs:subClassOf :NonFailStop ;
         rdfs:comment "A stuck-at failure happens when a device cannot change state, maintaining the same state despite changes in environment or actuation commands." .


###  https://w3id.org/iotf#Temperature
:Temperature rdf:type owl:Class ;
             rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#ThreadFault
:ThreadFault rdf:type owl:Class ;
             rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#Timing
:Timing rdf:type owl:Class ;
        rdfs:subClassOf :CommunicationFailure .


###  https://w3id.org/iotf#TotalOperationalTime
:TotalOperationalTime rdf:type owl:Class ;
                      rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#UpTime
:UpTime rdf:type owl:Class ;
        rdfs:subClassOf :PropertyValue .


###  https://w3id.org/iotf#UserInterfaceFault
:UserInterfaceFault rdf:type owl:Class ;
                    rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#VisualDisplayFault
:VisualDisplayFault rdf:type owl:Class ;
                    rdfs:subClassOf :HardwareFailure .


###  https://w3id.org/iotf#WIFIFault
:WIFIFault rdf:type owl:Class ;
           rdfs:subClassOf :RadioFault .


###  https://w3id.org/iotf#NetworkI/O
<https://w3id.org/iotf#NetworkI/O> rdf:type owl:Class ;
                                                              rdfs:subClassOf :PropertyValue .


###  https://saref.etsi.org/core/Device
<https://saref.etsi.org/core/Device> rdf:type owl:Class ;
                                     rdfs:subClassOf <https://saref.etsi.org/core/FeatureOfInterest> ,
                                                     <https://saref.etsi.org/saref4syst/System> ;
                                     rdfs:comment "A tangible object designed to accomplish a particular task. In order to accomplish this task, the device performs one or more functions. An instance of saref:Device represents one specific real world entity."@en ;
                                     rdfs:label "Device"@en ;
                                     <http://www.w3.org/2004/02/skos/core#example> "Examples of devices are a light switch, a temperature sensor, an energy meter, a water flow meter, and a laundry dryer. A laundry dryer is designed to dry laundry, and to accomplish this task it has a start/stop function."@en ,
                                                                                   "For example, a washing machine is designed to wash (task) and to accomplish this task it performs a start and stop function."@en ;
                                     <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Most axioms on saref:Device were useless and have been deleted."@en ,
                                                                                       "V3.2.1: definition of saref:Device clarifies it's a real world entity."@en ,
                                                                                       "V3.2.1: saref:Device is explicitly a subclass of saref:FeatureOfInterest and s4syst:System."@en .


###  https://saref.etsi.org/core/FeatureKind
<https://saref.etsi.org/core/FeatureKind> rdf:type owl:Class ;
                                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://www.w3.org/2004/02/skos/core#broader> ;
                                                            owl:allValuesFrom <https://saref.etsi.org/core/FeatureKind>
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://www.w3.org/2004/02/skos/core#narrower> ;
                                                            owl:allValuesFrom <https://saref.etsi.org/core/FeatureKind>
                                                          ] ;
                                          rdfs:comment "Feature kinds allow to describe kinds of features of interest, with common properties having the same value, and common states being the same. An instance of saref:FeatureKind represents an archetype of real world entities, for example to populate product catalogs."@en ;
                                          rdfs:label "Feature kinds"@en ;
                                          <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced class saref:FeatureKind"@en ,
                                                                                            "V3.2.1: Use SKOS to organize generic classes in taxonomies."@en .


###  https://saref.etsi.org/core/FeatureOfInterest
<https://saref.etsi.org/core/FeatureOfInterest> rdf:type owl:Class .


###  https://saref.etsi.org/core/PropertyValue
<https://saref.etsi.org/core/PropertyValue> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error1> ;
                                            rdfs:comment "Describes the value for a property. The property value is linked to its value expressed as an RDF literal (DP saref:hasValue), optionally to the unit of measurement (OP saref:isMeasuredIn), and optionally to the properties or properties of interest it is a value of (OP saref:isValueOfProperty)."@en ;
                                            rdfs:label "Property Value"@en ;
                                            <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:PropertyValue to simplify the association of a measure to a property, without always needing it to be a measurement made by a device."@en .


###  https://saref.etsi.org/saref4syst/System
<https://saref.etsi.org/saref4syst/System> rdf:type owl:Class .


